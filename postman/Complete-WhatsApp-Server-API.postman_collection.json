            },
            "url": {
              "raw": "{{baseUrl}}/api/templates/{{pendingTemplateId}}/reject",
              "host": ["{{baseUrl}}"],
              "path": ["api", "templates", "{{pendingTemplateId}}", "reject"]
            }
          }
        },
        {
          "name": "Delete Template",
          "request": {
            "method": "DELETE",
